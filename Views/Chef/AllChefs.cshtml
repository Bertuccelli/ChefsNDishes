@model List<Chef>

<a href="/chefs/new">Create a new Chef!</a>
<div>
    <h1>Check out our roster of Chefs!</h1>
    <table>
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th># of Dishes</th>
        </tr>
        @foreach (Chef chef in Model)
        {
            <tr>
                <td>@chef.FullName()</td>
                <td>@chef.DOB</td>
                <td>@chef.CreatedDishes.Count()</td>
            </tr>
        }
    </table>
</div>